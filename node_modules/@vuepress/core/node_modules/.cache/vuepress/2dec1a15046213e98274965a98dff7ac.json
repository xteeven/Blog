meConfig.logo\"\n        v-show=\"$isHome\"\n        :src=\"$themeConfig.logo\"\n        class=\"title-img\"\n      />\n      <span v-if=\"coverTitle\" class=\"title-text\">{{ coverTitle }}</span>\n    </h1>\n    <h2 class=\"cover-text\">{{ $site.description }}</h2>\n  </section>\n</template>\n\n<script>\nexport default {\n  name: 'HeaderCover',\n  props: {\n    item: {\n      type: String,\n      default: null\n    }\n  },\n  computed: {\n    coverTitle() {\n      const navItem = this.$themeConfig.nav.find(item => {\n        return item.link === this.$route.path\n      })\n\n      if (this.$isHome) {\n        return this.$themeConfig.logo ? false : this.$title\n      } else {\n        return (\n          (navItem && navItem.text) || this.$frontmatter.title || this.$title\n        )\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"stylus\" scoped>\n.header-cover\n  position absolute\n  top 0\n  bottom 0\n  left 0\n  right 0\n  width 100%\n  height 100%\n  background-size cover\n  background-position center\n  .cover-title\n    width 100%\n    text-align center\n    .title-img\n      max-width 100%\n      max-height ($coverHeight / 5)\n    .title-text\n      max-width 100%\n      max-height: ($coverHeight / 5)\n      font-size 4rem\n      color $whiteColor\n      overflow-wrap break-word\n  .cover-text\n    margin-top .5rem\n    font-size 1.2rem\n    color $whiteColor\n@media (max-width $phoneWidth)\n  .header-cover\n    .cover-title\n      .title-text\n        font-size 3rem\n</style>\n"],"sourceRoot":"node_modules/vuepress-theme-ououe/components"}]}